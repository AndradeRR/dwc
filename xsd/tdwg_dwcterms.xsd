    <xs:element name="DatasetID" type="dwc:nonEmptyString" substitutionGroup="dwc:anyIdentifier"/>
    <xs:group name="DatasetTerms">
        <xs:sequence>
        </xs:sequence>
    </xs:group>
    
    
